                            <% for(let row of userdetails){%>
                                <tr>
                                    <td><%- row._id%></td>
                                    <td><%- row.name%></td>
                                    <td><%- row.email%></td>
                                    <td><%- row.mobile%></td>
                                    <td><%- row.address%></td>
                                    <td><%- row.city%></td>
                                    <td><%- row.info%></td>
                                    <td>
                                     <% if(row.status) {%>
                                        <a href="/admin/manageuserstatus?s=block&regid=<%- row._id%>" title="block user">
                                        <img src="../images/block.jpg" height="90" width="90"/>
                                        </a>

                                        <%} else {%>
                                            <a href="/admin/manageuserstatus?s=unblock&regid=<%- row._id%>" title="unblock user">
                                                <img src="../images/unblock1.jpeg" height="90" width="90"/>
                                                </a>
                                        <%} %>
                                        </td>
                                    <td><a href="/admin/manageuserstatus?s=delete&regid=<%- row._id%>" title="delete">
                                        <img src="../images/del.jpeg" height="90" width="90"/>
                                        </a></td>
                                    </tr>
                                <% } %> 
                         </table>
                                                
