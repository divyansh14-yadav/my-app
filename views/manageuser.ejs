    <section id="about page" class="about page-wrapper">
        <div class="container">
                    <div style="margin-top:45px ;"  data-scroll-reveal="enter from the bottom after 0.3s" class="title text-center">
                        <h2>manageuser page</h2>
                         <table class="table table-bordered">
                            <tr>
                                <th>Reg id</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Mobile</th>
                                <th>Address</th>
                                <th>City</th>
                                <th>Info</th>
                                <th>Status</th>
                                <th>Action</th>
